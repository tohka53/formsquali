{
    "version": 2,
    "builds": [
      { 
        "src": "api/**/*.ts",
        "use": "@vercel/node"
      },
      { 
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/formsquali"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }